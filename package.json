{
  "name": "@sunruse-pwa/npm-template",
  "version": "999.999.999",
  "engines": {
    "node": ">=12.13.0"
  },
  "engineStrict": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/sunruse-pwa/npm-template"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jasmine": "3.7.7",
    "@typescript-eslint/eslint-plugin": "4.28.1",
    "@typescript-eslint/parser": "4.28.1",
    "del-cli": "3.0.1",
    "eslint": "7.23.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "jasmine": "3.7.0",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "prettier": "2.2.1",
    "typescript": "4.2.3"
  },
  "scripts": {
    "eslint": "eslint \"**/*.ts\"",
    "del-cli": "del-cli \"**/*.js\" \"**/*.d.ts\" \"!**/node_modules/**/*\"",
    "tsc": "tsc",
    "jasmine": "jasmine --config=unit.jasmine.json",
    "nyc": "nyc --check-coverage --all --exclude \"**/*.ts\" --exclude \"**/*.d.ts\" --exclude \"**/unit.*\" --exclude \"editor/index.js\" --exclude \"editor/webpack.config.js\" --lines 100 --functions 100 --branches 100 --statements 100 npm run-script jasmine",
    "test": "npm-run-all eslint del-cli tsc nyc"
  },
  "files": [
    "**/*.js",
    "**/*.d.ts",
    "!**/unit.js",
    "!**/unit.d.ts"
  ]
}
